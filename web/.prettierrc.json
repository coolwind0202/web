{
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 100,
  "importOrder": [
    "^@core/(.*)$",
    "react",
    "<THIRD_PARTY_MODULES>",
    "^@/(.*)$",
    "^@server/(.*)$",
    "^@ui/(.*)$",
    "^[./]",
    "(.*).module.css"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
